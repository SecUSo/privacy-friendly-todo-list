    <string name="welcome_title">Welcome</string>
    <string name="welcome_viewhelp">View Help</string>
    <string name="welcome_text">Welcome to the Privacy Friendly To-Do List. This application requires no permissions. You can:</string>
    <string name="welcome_hint_lists">Manage Tasks in labeled To-Do Lists.</string>
    <string name="welcome_hint_deadlines">Create deadlines and reminders for tasks.</string>
    <string name="welcome_hint_subtasks">Create subtasks by expanding the task.</string>
    <string name="welcome_hint_pin">Add a PIN-protection in the settings.</string>
